{
    "name": "phlib/mockfunction",
    "version": "1.0.0",
    "description": "Library to aid the testing of global functions, by creating an instance of the function in the namespace",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "James Dempster"
        }
    ],
    "require": {
        "php": "~5.4",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Phlib\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Phlib\\": "tests"
        }
    }
}
